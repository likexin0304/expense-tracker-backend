{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "alias": [
    "expense-tracker-api.vercel.app"
  ],
  "env": {
    "NODE_ENV": "production",
    "FORCE_REBUILD": "2025-10-17-config-endpoints"
  }
} 